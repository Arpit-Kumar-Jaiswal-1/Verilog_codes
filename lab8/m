#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55d5cd72f3a0 .scope module, "ex3_tb" "ex3_tb" 2 4;
 .timescale -9 -9;
v0x55d5cd740eb0_0 .var "clk", 0 0;
v0x55d5cd740f80_0 .net "q", 4 0, v0x55d5cd740c80_0;  1 drivers
v0x55d5cd741050_0 .var "rst", 0 0;
S_0x55d5cd72f530 .scope module, "uut" "ex3" 2 8, 3 1 0, S_0x55d5cd72f3a0;
 .timescale -9 -9;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 5 "q";
P_0x55d5cd72f710 .param/l "N" 0 3 1, +C4<00000000000000000000000000000101>;
v0x55d5cd6f5170_0 .net "clk", 0 0, v0x55d5cd740eb0_0;  1 drivers
v0x55d5cd740c80_0 .var "q", 4 0;
v0x55d5cd740d60_0 .net "rst", 0 0, v0x55d5cd741050_0;  1 drivers
E_0x55d5cd6f57a0 .event posedge, v0x55d5cd740d60_0, v0x55d5cd6f5170_0;
    .scope S_0x55d5cd72f530;
T_0 ;
    %wait E_0x55d5cd6f57a0;
    %load/vec4 v0x55d5cd740d60_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 5;
    %assign/vec4 v0x55d5cd740c80_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55d5cd740c80_0;
    %parti/s 1, 4, 4;
    %inv;
    %load/vec4 v0x55d5cd740c80_0;
    %parti/s 4, 1, 2;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x55d5cd740c80_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55d5cd72f3a0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d5cd740eb0_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x55d5cd740eb0_0;
    %inv;
    %store/vec4 v0x55d5cd740eb0_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x55d5cd72f3a0;
T_2 ;
    %vpi_call 2 16 "$dumpfile", "ex3.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55d5cd72f3a0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d5cd741050_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d5cd741050_0, 0, 1;
    %delay 200, 0;
    %vpi_call 2 21 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "exx3.v";
    "./ex3.v";
